	-- สร้างฟังก์ชันเพื่อเลื่อนปุ่มไปทางขวา
	local function moveButtonRight(button)
		-- ใช้ TweenService เพื่อทำให้การเลื่อนดูราบรื่น
		local tweenService = game:GetService("TweenService")
	
		-- กำหนดตำแหน่งใหม่ที่ต้องการให้เลื่อนไป
		local newPosition = UDim2.new(button.Position.X.Scale + 0.7, 0, button.Position.Y.Scale, 0) -- เลื่อนไปทางขวา 20% ของหน้าจอ
	
		-- กำหนด TweenInfo (ระยะเวลา 0.5 วินาที, การเคลื่อนไหวเป็นแบบ linear)
		local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
	
		-- สร้าง Tween และเล่นมัน
		local tween = tweenService:Create(button, tweenInfo, {Position = newPosition})
		tween:Play()
	end
	
	-- กำหนดให้ฟังก์ชันทำงานเมื่อกดปุ่ม
	script.Parent.Frame.Frame.Frame.on.MouseButton1Click:Connect(function()
		moveButtonRight(script.Parent.Frame.Frame.Frame.on) -- เลื่อนปุ่มไปทางขวา
		moveButtonRight(script.Parent.Frame.Frame.Frame.off) -- เลื่อนปุ่มไปทางขวา
		wait(1)
		script.Parent.Frame.Frame.Frame.on.Visible = false 	script.Parent.Frame.Frame.Frame.off.Visible = true
	
	end)
	local function moveButtonRight1(button1)
		-- ใช้ TweenService เพื่อทำให้การเลื่อนดูราบรื่น
		local tweenService = game:GetService("TweenService")
	
		-- กำหนดตำแหน่งใหม่ที่ต้องการให้เลื่อนไป
		local newPosition = UDim2.new(button1.Position.X.Scale + -0.7, 0, button1.Position.Y.Scale, 0) -- เลื่อนไปทางขวา 20% ของหน้าจอ
	
		-- กำหนด TweenInfo (ระยะเวลา 0.5 วินาที, การเคลื่อนไหวเป็นแบบ linear)
		local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
	
		-- สร้าง Tween และเล่นมัน
		local tween = tweenService:Create(button1, tweenInfo, {Position = newPosition})
		tween:Play()
	end
	
	-- กำหนดให้ฟังก์ชันทำงานเมื่อกดปุ่ม
	script.Parent.Frame.Frame.Frame.off.MouseButton1Click:Connect(function()
		moveButtonRight1(script.Parent.Frame.Frame.Frame.off) -- เลื่อนปุ่มไปทางขวา
		moveButtonRight1(script.Parent.Frame.Frame.Frame.on)
		wait(1)
		script.Parent.Frame.Frame.Frame.on.Visible = true 	script.Parent.Frame.Frame.Frame.off.Visible = false
	
	end)
	
	
	---------------------------------------------------------------------------------------------------------------------------------------------------
	---------------------------------------------------------------------------------------------------------------------------------------------------
	---------------------------------------------------------------------------------------------------------------------------------------------------
	------------------------------------------      -------------------       -------------------------------------------------------------------------------------
	---------------------------------------           -----------------      ----------  ---------------------------------------------------------------------------------
	---------------------------------------------------------------------------------------------------------------------------------------------------
	----------------------------------------------------              -----------------------------------------------------------------------------------------------
	--------------------------------------------------------         -------------------------------------------------------------------------------------------
	---------------------------------------------------------     ------------------------------------------------------------------------------------------
	---------------------------------------------------------------------------------------------------------------------------------------------------
	---------------------------------------------------------------------------------------------------------------------------------------------------
	local function moveFrameBack5(frame)
		-- ใช้ TweenService เพื่อทำให้การเลื่อนกลับไปตำแหน่งเดิมดูราบรื่น
		local tweenService = game:GetService("TweenService")
	
		-- กำหนดตำแหน่งเดิม {0.999, 0}, {0.828, 0}
		local originalPosition = UDim2.new(0.999, 0, 0.828, 0)
	
		-- กำหนด TweenInfo (ระยะเวลา 0.5 วินาที, การเคลื่อนไหวเป็นแบบ linear)
		local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
	
		-- สร้าง Tween และเล่นมัน
		local tween = tweenService:Create(frame, tweenInfo, {Position = originalPosition})
		tween:Play()
		wait(0.5)
		script.Parent.CH.Visible = false
	
	end
	
	local function moveFrameToNewPosition(frame)
		-- ใช้ TweenService เพื่อทำให้การเลื่อนดูราบรื่น
		local tweenService = game:GetService("TweenService")
	
		-- กำหนดตำแหน่งใหม่ที่ต้องการให้เลื่อนไป (เปลี่ยนเป็น {0.864, 0}, {0.828, 0})
		local newPosition = UDim2.new(0.864, 0, 0.828, 0)
	
		-- กำหนด TweenInfo (ระยะเวลา 0.5 วินาที, การเคลื่อนไหวเป็นแบบ linear)
		local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
	
		-- สร้าง Tween และเล่นมัน
		local tween = tweenService:Create(frame, tweenInfo, {Position = newPosition})
		script.Parent.CH.Visible = true
		tween:Play()
	
		-- ตั้งเวลา 5 วินาที แล้วเลื่อนกลับไปที่เดิม
		delay(3, function()
			moveFrameBack5(frame)
		end)
	end
	
	-- ฟังก์ชันเลื่อน Frame กลับไปยังตำแหน่งเริ่มต้น
	
	
	-- ตั้งค่า Position เริ่มต้นของ Frame เป็น {0.999, 0}, {0.828, 0}
	script.Parent.CH.Visible = false
	script.Parent.CH.Position = UDim2.new(0.999, 0, 0.828, 0)
	
	-- เมื่อกดปุ่มให้เลื่อนไปยังตำแหน่งใหม่
	script.Parent.Frame.Frame.Frame.on.MouseButton1Click:Connect(function()
		moveFrameToNewPosition(script.Parent.CH)
	end)
	
