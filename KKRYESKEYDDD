		local tween = tweenService:Create(frame, tweenInfo, {Size = targetSize})
		local twe = tweenService:Create(tb1, tweenInfo, {Size = targetSize1})
		local twe2 = tweenService:Create(tb2, tweenInfo, {Size = targetSize1})
		local twe3 = tweenService:Create(tb3, tweenInfo, {Size = targetSize1})
		local twe4 = tweenService:Create(tb4, tweenInfo, {Size = targetSize1})
	
	
	
		tween:Play() -- ‡πÄ‡∏•‡πà‡∏ô‡∏Å‡∏≤‡∏£‡∏Ç‡∏¢‡∏≤‡∏¢‡∏Ç‡∏ô‡∏≤‡∏î
