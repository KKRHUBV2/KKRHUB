	local function moveButtonRight(button)
		-- ‡πÉ‡∏ä‡πâ TweenService ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡∏Å‡∏≤‡∏£‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏î‡∏π‡∏£‡∏≤‡∏ö‡∏£‡∏∑‡πà‡∏ô
		local tweenService = game:GetService("TweenService")
	
		-- ‡∏Å‡∏≥‡∏´‡∏ô‡∏î‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÉ‡∏´‡∏°‡πà‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡πÉ‡∏´‡πâ‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡πÑ‡∏õ
		local newPosition = UDim2.new(button.Position.X.Scale + 0.7, 0, button.Position.Y.Scale, 0) -- ‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡πÑ‡∏õ‡∏ó‡∏≤‡∏á‡∏Ç‡∏ß‡∏≤ 20% ‡∏Ç‡∏≠‡∏á‡∏´‡∏ô‡πâ‡∏≤‡∏à‡∏≠
	
		-- ‡∏Å‡∏≥‡∏´‡∏ô‡∏î TweenInfo (‡∏£‡∏∞‡∏¢‡∏∞‡πÄ‡∏ß‡∏•‡∏≤ 0.5 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ, ‡∏Å‡∏≤‡∏£‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡πÑ‡∏´‡∏ß‡πÄ‡∏õ‡πá‡∏ô‡πÅ‡∏ö‡∏ö linear)
		local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
	
		-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Tween ‡πÅ‡∏•‡∏∞‡πÄ‡∏•‡πà‡∏ô‡∏°‡∏±‡∏ô
		local tween = tweenService:Create(button, tweenInfo, {Position = newPosition})
		tween:Play()
	end
	
	-- ‡∏Å‡∏≥‡∏´‡∏ô‡∏î‡πÉ‡∏´‡πâ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°
	script.Parent.Frame.Frame.Frame.on.MouseButton1Click:Connect(function()
		moveButtonRight(script.Parent.Frame.Frame.Frame.on) -- ‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏õ‡∏∏‡πà‡∏°‡πÑ‡∏õ‡∏ó‡∏≤‡∏á‡∏Ç‡∏ß‡∏≤
		moveButtonRight(script.Parent.Frame.Frame.Frame.off) -- ‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏õ‡∏∏‡πà‡∏°‡πÑ‡∏õ‡∏ó‡∏≤‡∏á‡∏Ç‡∏ß‡∏≤
		wait(1)
		script.Parent.Frame.Frame.Frame.on.Visible = false 	script.Parent.Frame.Frame.Frame.off.Visible = true
	
	end)
	local function moveButtonRight1(button1)
		-- ‡πÉ‡∏ä‡πâ TweenService ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡∏Å‡∏≤‡∏£‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏î‡∏π‡∏£‡∏≤‡∏ö‡∏£‡∏∑‡πà‡∏ô
		local tweenService = game:GetService("TweenService")
	
		-- ‡∏Å‡∏≥‡∏´‡∏ô‡∏î‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÉ‡∏´‡∏°‡πà‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡πÉ‡∏´‡πâ‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡πÑ‡∏õ
		local newPosition = UDim2.new(button1.Position.X.Scale + -0.7, 0, button1.Position.Y.Scale, 0) -- ‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡πÑ‡∏õ‡∏ó‡∏≤‡∏á‡∏Ç‡∏ß‡∏≤ 20% ‡∏Ç‡∏≠‡∏á‡∏´‡∏ô‡πâ‡∏≤‡∏à‡∏≠
	
		-- ‡∏Å‡∏≥‡∏´‡∏ô‡∏î TweenInfo (‡∏£‡∏∞‡∏¢‡∏∞‡πÄ‡∏ß‡∏•‡∏≤ 0.5 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ, ‡∏Å‡∏≤‡∏£‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡πÑ‡∏´‡∏ß‡πÄ‡∏õ‡πá‡∏ô‡πÅ‡∏ö‡∏ö linear)
		local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
	
		-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Tween ‡πÅ‡∏•‡∏∞‡πÄ‡∏•‡πà‡∏ô‡∏°‡∏±‡∏ô
		local tween = tweenService:Create(button1, tweenInfo, {Position = newPosition})
		tween:Play()
	end
	
	-- ‡∏Å‡∏≥‡∏´‡∏ô‡∏î‡πÉ‡∏´‡πâ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°
	script.Parent.Frame.Frame.Frame.off.MouseButton1Click:Connect(function()
		moveButtonRight1(script.Parent.Frame.Frame.Frame.off) -- ‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏õ‡∏∏‡πà‡∏°‡πÑ‡∏õ‡∏ó‡∏≤‡∏á‡∏Ç‡∏ß‡∏≤
		moveButtonRight1(script.Parent.Frame.Frame.Frame.on)
		wait(1)
		script.Parent.Frame.Frame.Frame.on.Visible = true 	script.Parent.Frame.Frame.Frame.off.Visible = false
	
	end)
	
	
