local originalDurations = {}

-- ฟังก์ชันปรับ HoldDuration เป็น 0 สำหรับทุก ProximityPrompt
local function setProximityPromptDurationToZero()
    for _, prompt in ipairs(workspace:GetDescendants()) do
        if prompt:IsA("ProximityPrompt") then
            -- เก็บค่าเดิมของ HoldDuration ถ้ายังไม่ได้เก็บ
            if not originalDurations[prompt] then
                originalDurations[prompt] = prompt.HoldDuration
            end
            -- ตั้งค่า HoldDuration เป็น 0 และปิด RequiresLineOfSight
            prompt.HoldDuration = 0
            prompt.RequiresLineOfSight = false
        end
    end
end

-- ฟังก์ชันปรับ HoldDuration กลับเป็นค่าเดิม
local function resetProximityPromptDuration()
    for _, prompt in ipairs(workspace:GetDescendants()) do
        if prompt:IsA("ProximityPrompt") then
            -- ถ้ามีการเก็บค่าเดิมไว้ ก็ปรับกลับ
            if originalDurations[prompt] then
                prompt.HoldDuration = originalDurations[prompt]
            end
        end
    end
end

-- เมื่อกด Button1 ให้ปรับ HoldDuration เป็น 0
script.Parent.Frame.Frame.Frame.on.MouseButton1Click:Connect(function()
    setProximityPromptDurationToZero()
end)

-- เมื่อกด Button2 ให้ปรับ HoldDuration กลับเป็นค่าเดิม
script.Parent.Frame.Frame.Frame.off.MouseButton1Click:Connect(function()
    resetProximityPromptDuration()
end)


