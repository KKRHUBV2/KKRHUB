--By KKRHUB
local count = 0

for _, part in pairs(workspace:GetDescendants()) do
    if part:IsA("BasePart") and part.Color == Color3.fromRGB(255, 0, 0) then
        local sound = part:FindFirstChildOfClass("Sound")
        local script = part:FindFirstChildOfClass("Script")
        local touchInterest = part:FindFirstChild("TouchInterest")
        
        if sound and script and touchInterest then
            count = count + 1
            part:Destroy()  -- ลบ Part ที่ตรงเงื่อนไข
        end
    end
end


