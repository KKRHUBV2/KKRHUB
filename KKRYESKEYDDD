	--Auto Farm
	local MAuto = me.Parent.MenuFrame2
local running = false -- ตัวแปรควบคุมลูป

MAuto.autoF1on.MouseButton1Click:Connect(function()
    MAuto.autoF1off.Visible = true
    MAuto.autoF1on.Visible = false
    script.Parent.Bye.byr.TextLabel.Text = "ออโต้เก็บขยะ🤑"
    moveFrameToNewPosition4(script.Parent.Bye)

    running = true

    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()

    -- ตรวจสอบว่า PrimaryPart ของตัวละครถูกตั้งค่าเป็น "HumanoidRootPart"
    character.PrimaryPart = character:WaitForChild("HumanoidRootPart")

    while running do
        local found = false
        for _, bag in pairs(workspace:GetChildren()) do
            if bag.Name == "TrashBag" then
                local prompt = bag:FindFirstChild("QueueInwZa007")
                if prompt and prompt.Enabled then
                    if character and character.PrimaryPart and bag.PrimaryPart then
                        -- เคลื่อนที่ตัวละครไปที่ Bag โดยการตั้ง CFrame ของ HumanoidRootPart
                        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
                        humanoidRootPart.CFrame = bag.CFrame -- ใช้ CFrame ของ Bag โดยตรง

                        task.wait(0.2)

                        -- ตรวจสอบว่า prompt ทำงาน
                        if prompt then
                            prompt:InputHoldBegin()
                            task.wait(prompt.HoldDuration or 0)
                            prompt:InputHoldEnd()
                        else
                            warn("ไม่พบ ProximityPrompt ใน Bag!")
                        end
                        found = true
                        break
                    else
                        warn("PrimaryPart ของ Character หรือ Bag ไม่ได้ถูกตั้งค่า!")
                    end
                end
            end
        end
        if not found then
            MAuto.autoF1off.Visible = false
            MAuto.autoF1on.Visible = true
            running = false
            script.Parent.Bye.byr.TextLabel.Text = "หยุดทำงานหาขยะไม่เจอ"
            moveFrameToNewPosition4(script.Parent.Bye)
            break
        end
        task.wait(1)
    end
end)

MAuto.autoF1off.MouseButton1Click:Connect(function()
    MAuto.autoF1off.Visible = false
    MAuto.autoF1on.Visible = true
    running = false
end)


	-----------------------------------------------------------------------
	
local running2 = false
local noclipEnabled = false  -- ตัวแปรควบคุมการเปิด/ปิด noclip

-- ฟังก์ชันสำหรับเปิด/ปิด noclip
function toggleNoclip(enable)
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    local humanoid = character:FindFirstChild("Humanoid")

    if not humanoid or not humanoidRootPart then return end

    if enable then
        -- เปิด noclip
        humanoidRootPart.CanCollide = false  -- ไม่ให้ตัวละครชนกับวัตถุ
        humanoid.PlatformStand = true  -- ทำให้ตัวละครอยู่ในโหมดที่ไม่สามารถชนกับพื้น
    else
        -- ปิด noclip
        humanoidRootPart.CanCollide = true  -- ให้ตัวละครชนกับวัตถุ
        humanoid.PlatformStand = false  -- ปลดล็อคการใช้ platform stand
    end
end

-- กดปุ่ม `autoF1on2` เพื่อเริ่มออโต้ (กดแค่ครั้งเดียว)
MAuto.autoF1on2.MouseButton1Click:Connect(function()
    if running2 then return end -- ป้องกันการกดซ้ำ
    running2 = true

    MAuto.autoF1off2.Visible = true
    MAuto.autoF1on2.Visible = false
    script.Parent.Bye.byr.TextLabel.Text = "ออโต้เก็บขยะ🤑"
    moveFrameToNewPosition4(script.Parent.Bye)

    -- เปิดใช้งาน noclip
    toggleNoclip(true)

    -- **ใช้ `task.spawn()` เพื่อทำให้โค้ดทำงานต่อเนื่องโดยไม่ต้องกดปุ่มซ้ำ**
    task.spawn(function()
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
        local humanoid = character:FindFirstChild("Humanoid")

        local bagList = {}

        while running2 do
            local found = false

            for _, bag in pairs(workspace:GetChildren()) do
                if bag.Name == "TrashBag" and not table.find(bagList, bag) then
                    local prompt = bag:FindFirstChild("QueueInwZa007")
                    if prompt and prompt.Enabled then
                        if humanoidRootPart and humanoid then
                            local targetCFrame = bag:IsA("Model") and bag.PrimaryPart and bag.PrimaryPart.CFrame or bag.CFrame

                            humanoidRootPart.CFrame = targetCFrame * CFrame.new(0, -10, 0)
                            task.wait(0.2)

                            humanoidRootPart.Anchored = true

                            while humanoidRootPart.Position.Y > -100 and running2 do
                                humanoidRootPart.CFrame = targetCFrame * CFrame.new(0, -10, 0)
                                task.wait(0.1)
                            end

                            -- กด ProximityPrompt อัตโนมัติ
                            prompt:InputHoldBegin()
                            task.wait(prompt.HoldDuration or 0)
                            prompt:InputHoldEnd()

                            humanoidRootPart.Anchored = false

                            table.insert(bagList, bag)

                            found = true

                            -- ตรวจสอบทีมของผู้เล่นแล้ววาร์ปไปตำแหน่งที่เหมาะสม
                            if player.Team then
                                if player.Team.Name == "บ้านฝั่งคลองเตย" then
                                    -- วาร์ปไปยังจุดของทีม "บ้านฝั่งคลองเตย"
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1146.37036, 2.90202761, -399.228149, 0, 0, 1, 0, 1, 0, -1, 0, 0)
                                elseif player.Team.Name == "บ้านฝั่งธน" then
                                    -- วาร์ปไปยังจุดของทีม "บ้านฝั่งธน"
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1069.29102, 2.90202761, -403.238678, -0.0349013805, 0, 0.999390781, 0, 1, 0, -0.999390781, 0, -0.0349013805)
                                end
                            end
                        end
                    end
                end
            end

            if not found then
                MAuto.autoF1off2.Visible = false
                MAuto.autoF1on2.Visible = true
                running2 = false
                toggleNoclip(false)

                script.Parent.Bye.byr.TextLabel.Text = "หยุดทำงานหาขยะไม่เจอ"
                moveFrameToNewPosition4(script.Parent.Bye)
                break
            end

            task.wait(0.1)
        end
    end)
end)

-- ปุ่มหยุด
MAuto.autoF1off2.MouseButton1Click:Connect(function()
    toggleNoclip(false)

    MAuto.autoF1off2.Visible = false
    MAuto.autoF1on2.Visible = true
    running2 = false

    local player = game.Players.LocalPlayer  -- ตรวจสอบผู้เล่น

    -- เช็คทีมของผู้เล่นและวาร์ปไปยังจุดที่กำหนด
    if player.Team then
        if player.Team.Name == "บ้านฝั่งคลองเตย" then
            -- วาร์ปไปยังจุดของทีม "บ้านฝั่งคลองเตย"
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1146.37036, 2.90202761, -399.228149, 0, 0, 1, 0, 1, 0, -1, 0, 0)
        elseif player.Team.Name == "บ้านฝั่งธน" then
            -- วาร์ปไปยังจุดของทีม "บ้านฝั่งธน"
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1069.29102, 2.90202761, -403.238678, -0.0349013805, 0, 0.999390781, 0, 1, 0, -0.999390781, 0, -0.0349013805)
        end
    end
end)

	---------------------------------------------------------
	
local F1 = me.Parent.Frame1
local player = game.Players.LocalPlayer
local openButton = F1.open
local closeButton = F1.off

local function adjustSize(part, size)
    if part then
        part.Size = size
    end
end

openButton.MouseButton1Click:Connect(function()
    -- ดึงค่าจาก TextBox ทุกครั้งที่กดปุ่ม
    local hitbox = tonumber(F1.TextBoxsize.Text)  -- แปลงเป็นตัวเลข
    if not hitbox then
        warn("ค่าที่กรอกไม่ถูกต้อง")
        return
    end

    for _, otherPlayer in pairs(game.Players:GetPlayers()) do
        if otherPlayer ~= player then
            local otherCharacter = otherPlayer.Character
            if otherCharacter then
                local otherHumanoidRootPart = otherCharacter:FindFirstChild("HumanoidRootPart")
                if otherHumanoidRootPart then
                    otherHumanoidRootPart.Transparency = 0.8
                    adjustSize(otherHumanoidRootPart, Vector3.new(hitbox, hitbox, hitbox))
                end
            end
        end
    end
end)

closeButton.MouseButton1Click:Connect(function()
    for _, otherPlayer in pairs(game.Players:GetPlayers()) do
        if otherPlayer ~= player then
            local otherCharacter = otherPlayer.Character
            if otherCharacter then
                local otherHumanoidRootPart = otherCharacter:FindFirstChild("HumanoidRootPart")
                if otherHumanoidRootPart then
                    otherHumanoidRootPart.Transparency = 1
                    adjustSize(otherHumanoidRootPart, Vector3.new(2, 2, 1))
                end
            end
        end
    end
end)

	
	
end
coroutine.wrap(LTVFGO_fake_script)()
