	local me = script.Parent.Frame.MenuFrame
	me.Godmod.MouseButton1Click:Connect(function()
		me.Godmod.Visible = false
		me.Godmodoff.Visible = true
		script.Parent.Bye.byr.TextLabel.Text = "‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô Godmodüòà"
		moveFrameToNewPosition4(script.Parent.Bye)
		local safeZone = workspace:WaitForChild("Safezone") safeZone.Size = Vector3.new(700, 11, 600)
	end)
	
	me.Godmodoff.MouseButton1Click:Connect(function()
		me.Godmod.Visible = true
		me.Godmodoff.Visible = false
		script.Parent.Bye.byr.TextLabel.Text = "‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô Godmodüòá"
		moveFrameToNewPosition4(script.Parent.Bye)
	local safeZone = workspace:WaitForChild("Safezone") safeZone.Size = Vector3.new(34.68862533569336, 10.279996871948242, 62.766056060791016)
	end)

	me.Vip.MouseButton1Click:Connect(function()
	
		script.Parent.Bye.byr.TextLabel.Text = "‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô Vipü§ë"
		moveFrameToNewPosition4(script.Parent.Bye)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1203.86731, 3.30128479, -52.7147942, 0.0723029971, 4.74948258e-09, -0.9973827, -3.11800434e-08, 1, 2.50161958e-09, 0.9973827, 3.09175618e-08, 0.0723029971)
	end)
	
	me.TP.MouseButton1Click:Connect(function()
		script.Parent.Bye.byr.TextLabel.Text = "‡∏´‡∏ô‡∏µüòé"
		moveFrameToNewPosition4(script.Parent.Bye)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1121.86292, 3.99999952, -427.973114, -0.918227911, -1.03538271e-08, -0.39605242, 4.05657374e-08, 1, -1.20192226e-07, 0.39605242, -1.2643001e-07, -0.918227911)
	end)
	
	me.Speed.MouseButton1Click:Connect(function()
		script.Parent.Bye.byr.TextLabel.Text = "‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ßü§´"
		moveFrameToNewPosition4(script.Parent.Bye)
		local player = game.Players.LocalPlayer
		local character = player.Character or player.CharacterAdded:Wait()
		local humanoid = character:WaitForChild("Humanoid")
		game:GetService("RunService").Heartbeat:Connect(function()
			if humanoid.WalkSpeed ~= 30 then
				humanoid.WalkSpeed = 30
			end
		end)
	end)
	
	
	
	
	
	
	--------------------------------------------------------
	local function moveFrameBack77(frame)
		-- ‡πÉ‡∏ä‡πâ TweenService ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡∏Å‡∏≤‡∏£‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏Å‡∏•‡∏±‡∏ö‡πÑ‡∏õ‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏î‡∏¥‡∏°‡∏î‡∏π‡∏£‡∏≤‡∏ö‡∏£‡∏∑‡πà‡∏ô
		local tweenService = game:GetService("TweenService")
	
		-- ‡∏Å‡∏≥‡∏´‡∏ô‡∏î‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏î‡∏¥‡∏° {0.999, 0}, {0.828, 0}
		local originalPosition = UDim2.new(0.999, 0, 0.828, 0)
	
		-- ‡∏Å‡∏≥‡∏´‡∏ô‡∏î TweenInfo (‡∏£‡∏∞‡∏¢‡∏∞‡πÄ‡∏ß‡∏•‡∏≤ 0.5 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ, ‡∏Å‡∏≤‡∏£‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡πÑ‡∏´‡∏ß‡πÄ‡∏õ‡πá‡∏ô‡πÅ‡∏ö‡∏ö linear)
		local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
	
		-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Tween ‡πÅ‡∏•‡∏∞‡πÄ‡∏•‡πà‡∏ô‡∏°‡∏±‡∏ô
		local tween = tweenService:Create(frame, tweenInfo, {Position = originalPosition})
		tween:Play()
		wait(0.5)
		script.Parent.CH.Visible = false
	
	end
	
	local function moveFrameToNewPosition77(frame)
		-- ‡πÉ‡∏ä‡πâ TweenService ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡∏Å‡∏≤‡∏£‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏î‡∏π‡∏£‡∏≤‡∏ö‡∏£‡∏∑‡πà‡∏ô
		local tweenService = game:GetService("TweenService")
	
		-- ‡∏Å‡∏≥‡∏´‡∏ô‡∏î‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÉ‡∏´‡∏°‡πà‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡πÉ‡∏´‡πâ‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡πÑ‡∏õ (‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÄ‡∏õ‡πá‡∏ô {0.864, 0}, {0.828, 0})
		local newPosition = UDim2.new(0.864, 0, 0.828, 0)
	
		-- ‡∏Å‡∏≥‡∏´‡∏ô‡∏î TweenInfo (‡∏£‡∏∞‡∏¢‡∏∞‡πÄ‡∏ß‡∏•‡∏≤ 0.5 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ, ‡∏Å‡∏≤‡∏£‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡πÑ‡∏´‡∏ß‡πÄ‡∏õ‡πá‡∏ô‡πÅ‡∏ö‡∏ö linear)
		local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
	
		-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Tween ‡πÅ‡∏•‡∏∞‡πÄ‡∏•‡πà‡∏ô‡∏°‡∏±‡∏ô
		local tween = tweenService:Create(frame, tweenInfo, {Position = newPosition})
		script.Parent.CH.Visible = true
		tween:Play()
	
		-- ‡∏ï‡∏±‡πâ‡∏á‡πÄ‡∏ß‡∏•‡∏≤ 5 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ ‡πÅ‡∏•‡πâ‡∏ß‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏Å‡∏•‡∏±‡∏ö‡πÑ‡∏õ‡∏ó‡∏µ‡πà‡πÄ‡∏î‡∏¥‡∏°
		delay(3, function()
			moveFrameBack77(frame)
		end)
	end
	
	-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô Frame ‡∏Å‡∏•‡∏±‡∏ö‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô
	
	
	-- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ Position ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡∏Ç‡∏≠‡∏á Frame ‡πÄ‡∏õ‡πá‡∏ô {0.999, 0}, {0.828, 0}
	script.Parent.CH.Visible = false
	script.Parent.CH.Position = UDim2.new(0.999, 0, 0.828, 0)
	
	-- ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°‡πÉ‡∏´‡πâ‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÉ‡∏´‡∏°‡πà
	button.MouseButton1Click:Connect(function()
		moveFrameToNewPosition77(script.Parent.CH)
	end)
	
	
	--------------------------------------------------------------------------
	
	local player = game.Players.LocalPlayer
	local backpack = player:WaitForChild("Backpack")
	local ragTapeFrame = me.Parent.Frame1.RagTap
	
	-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏ä‡πá‡∏Ñ‡πÅ‡∏•‡∏∞‡∏ô‡∏±‡∏ö‡∏à‡∏≥‡∏ô‡∏ß‡∏ô Rag ‡πÅ‡∏•‡∏∞ Tape
	local function updateRagTapeCount()
		local ragCount = 0
		local tapeCount = 0
	
		-- ‡πÄ‡∏ä‡πá‡∏Ñ‡πÉ‡∏ô‡∏Å‡∏£‡∏∞‡πÄ‡∏õ‡πã‡∏≤‡∏Ç‡∏≠‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
		if backpack then
			for _, tool in pairs(backpack:GetChildren()) do
				if tool:IsA("Tool") then
					if tool.Name == "Rag" then
						ragCount = ragCount + 1
					elseif tool.Name == "Tape" then
						tapeCount = tapeCount + 1
					end
				end
			end
		end
	
		-- ‡πÅ‡∏™‡∏î‡∏á‡∏à‡∏≥‡∏ô‡∏ß‡∏ô Rag ‡πÅ‡∏•‡∏∞ Tape ‡πÉ‡∏ô Frame1.RagTap
		ragTapeFrame.Text = ragCount .. "/" .. tapeCount
	end
	
	-- ‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡πÉ‡∏ä‡πâ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏à‡∏≥‡∏ô‡∏ß‡∏ô
	updateRagTapeCount()
	
	-- ‡∏ñ‡πâ‡∏≤‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡πÉ‡∏´‡πâ‡∏°‡∏±‡∏ô‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏°‡∏µ‡∏Å‡∏≤‡∏£‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÅ‡∏õ‡∏•‡∏á‡πÉ‡∏ô‡∏Å‡∏£‡∏∞‡πÄ‡∏õ‡πã‡∏≤ (‡∏ñ‡πâ‡∏≤‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏´‡∏£‡∏∑‡∏≠‡∏•‡∏ö Tool)
	backpack.ChildAdded:Connect(updateRagTapeCount)
	backpack.ChildRemoved:Connect(updateRagTapeCount)
	
	-------------------------------------------------------------------------------------------
	-------------------------------------------------------------------------------------------
	-------------------------------------------------------------------------------------------
	-------------------------------------------------------------------------------------------
	-------------------------------------------------------------------------------------------
