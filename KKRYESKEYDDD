local function LTVFGO_fake_script() -- KKRHUB3.LocalScript 
	local script = Instance.new('LocalScript', KKRHUB3)

	script.Parent.lego.Draggable = true
	script.Parent.lego.Active = true
	script.Parent.Frame.exit.l.MouseButton1Click:Connect(function()
		script.Parent.lego.Visible = true
		script.Parent.Frame.Visible = false
	end)
	script.Parent.lego.ImageButton.MouseButton1Click:Connect(function()
		script.Parent.lego.Visible = false
		script.Parent.Frame.Visible = true
	end)
	
	
	script.Parent.Frame.Draggable = true
	script.Parent.Frame.Active = true
	
	local button = script.Parent.Frame.BB.a 
	local bu = button.Parent.a2
	local frame = script.Parent.Frame:WaitForChild("Menu")
	local tweenService = game:GetService("TweenService")
	local tb1 = frame.TextButton1
	local tb2 = frame.TextButton2
	local tb3 = frame.TextButton3
	local tb4 = frame.TextButton4
	
	---------------------------------------------------------------------------------------------------------------------------------------------------
	--[[
	Text
	]]--
	local tt1 = "ü§ìCooldown 0 sec."
	local tt2 = "üëëHitbox / ‡πÄ‡∏ä‡πá‡∏Ñ‡∏Ç‡∏≠‡∏á"
	local tt3 = "üü•Hack"
	local tt4 = "üë®‚ÄçüåæAuto Farm"
	
	
	
	
	
	
	
	
	
	
	
	
	
	---------------------------------------------------------------------------------------------------------------------------------------------------
	tb1.MouseButton1Click:Connect(function()
		script.Parent.Frame.Frame.Visible = true
		script.Parent.Frame.Frame1.Visible = false
		script.Parent.Frame.MenuFrame.Visible = false
	script.Parent.Frame.MenuFrame2.Visible = false
	
	end)
	tb2.MouseButton1Click:Connect(function()
		script.Parent.Frame.Frame.Visible = false
		script.Parent.Frame.Frame1.Visible = true
		script.Parent.Frame.MenuFrame.Visible = false
		script.Parent.Frame.MenuFrame2.Visible = false
	
	end)
