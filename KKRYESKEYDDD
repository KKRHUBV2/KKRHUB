	-------------------------------------------------------------------------------------------
	-------------------------------------------------------------------------------------------
	
	--Auto Farm
	local MAuto = me.Parent.MenuFrame2
local running = false -- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏Ñ‡∏ß‡∏ö‡∏Ñ‡∏∏‡∏°‡∏•‡∏π‡∏õ

MAuto.autoF1on.MouseButton1Click:Connect(function()
    MAuto.autoF1off.Visible = true
    MAuto.autoF1on.Visible = false
    script.Parent.Bye.byr.TextLabel.Text = "‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡πÄ‡∏Å‡πá‡∏ö‡∏Ç‡∏¢‡∏∞ü§ë"
    moveFrameToNewPosition4(script.Parent.Bye)

    running = true

    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()

    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤ PrimaryPart ‡∏Ç‡∏≠‡∏á‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏ñ‡∏π‡∏Å‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡πÄ‡∏õ‡πá‡∏ô "HumanoidRootPart"
    character.PrimaryPart = character:WaitForChild("HumanoidRootPart")

    while running do
        local found = false
        for _, bag in pairs(workspace:GetChildren()) do
            if bag.Name == "TrashBag" then
                local prompt = bag:FindFirstChild("QueueInwZa007")
                if prompt and prompt.Enabled then
                    if character and character.PrimaryPart and bag.PrimaryPart then
                        -- ‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÑ‡∏õ‡∏ó‡∏µ‡πà Bag ‡πÇ‡∏î‡∏¢‡∏Å‡∏≤‡∏£‡∏ï‡∏±‡πâ‡∏á CFrame ‡∏Ç‡∏≠‡∏á HumanoidRootPart
                        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
                        humanoidRootPart.CFrame = bag.CFrame -- ‡πÉ‡∏ä‡πâ CFrame ‡∏Ç‡∏≠‡∏á Bag ‡πÇ‡∏î‡∏¢‡∏ï‡∏£‡∏á

                        task.wait(0.2)

                        -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤ prompt ‡∏ó‡∏≥‡∏á‡∏≤‡∏ô
                        if prompt then
                            prompt:InputHoldBegin()
                            task.wait(prompt.HoldDuration or 0)
                            prompt:InputHoldEnd()
                        else
                            warn("‡πÑ‡∏°‡πà‡∏û‡∏ö ProximityPrompt ‡πÉ‡∏ô Bag!")
                        end
                        found = true
                        break
                    else
                        warn("PrimaryPart ‡∏Ç‡∏≠‡∏á Character ‡∏´‡∏£‡∏∑‡∏≠ Bag ‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ‡∏ñ‡∏π‡∏Å‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤!")
                    end
                end
            end
        end
        if not found then
            MAuto.autoF1off.Visible = false
            MAuto.autoF1on.Visible = true
            running = false
            script.Parent.Bye.byr.TextLabel.Text = "‡∏´‡∏¢‡∏∏‡∏î‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏´‡∏≤‡∏Ç‡∏¢‡∏∞‡πÑ‡∏°‡πà‡πÄ‡∏à‡∏≠"
            moveFrameToNewPosition4(script.Parent.Bye)
            break
        end
        task.wait(1)
    end
end)

MAuto.autoF1off.MouseButton1Click:Connect(function()
    MAuto.autoF1off.Visible = false
    MAuto.autoF1on.Visible = true
    running = false
end)


	-----------------------------------------------------------------------
	
local running2 = false
local noclipEnabled = false  -- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏Ñ‡∏ß‡∏ö‡∏Ñ‡∏∏‡∏°‡∏Å‡∏≤‡∏£‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î noclip

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î noclip
function toggleNoclip(enable)
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    local humanoid = character:FindFirstChild("Humanoid")

    if not humanoid or not humanoidRootPart then return end

    if enable then
        -- ‡πÄ‡∏õ‡∏¥‡∏î noclip
        humanoidRootPart.CanCollide = false  -- ‡πÑ‡∏°‡πà‡πÉ‡∏´‡πâ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏ä‡∏ô‡∏Å‡∏±‡∏ö‡∏ß‡∏±‡∏ï‡∏ñ‡∏∏
        humanoid.PlatformStand = true  -- ‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô‡πÇ‡∏´‡∏°‡∏î‡∏ó‡∏µ‡πà‡πÑ‡∏°‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡∏ä‡∏ô‡∏Å‡∏±‡∏ö‡∏û‡∏∑‡πâ‡∏ô
    else
        -- ‡∏õ‡∏¥‡∏î noclip
        humanoidRootPart.CanCollide = true  -- ‡πÉ‡∏´‡πâ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏ä‡∏ô‡∏Å‡∏±‡∏ö‡∏ß‡∏±‡∏ï‡∏ñ‡∏∏
        humanoid.PlatformStand = false  -- ‡∏õ‡∏•‡∏î‡∏•‡πá‡∏≠‡∏Ñ‡∏Å‡∏≤‡∏£‡πÉ‡∏ä‡πâ platform stand
    end
end

-- ‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏° `autoF1on2` ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏≠‡∏≠‡πÇ‡∏ï‡πâ (‡∏Å‡∏î‡πÅ‡∏Ñ‡πà‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡πÄ‡∏î‡∏µ‡∏¢‡∏ß)
MAuto.autoF1on2.MouseButton1Click:Connect(function()
    if running2 then return end -- ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏Å‡∏≤‡∏£‡∏Å‡∏î‡∏ã‡πâ‡∏≥
    running2 = true

    MAuto.autoF1off2.Visible = true
    MAuto.autoF1on2.Visible = false
    script.Parent.Bye.byr.TextLabel.Text = "‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡πÄ‡∏Å‡πá‡∏ö‡∏Ç‡∏¢‡∏∞ü§ë"
    moveFrameToNewPosition4(script.Parent.Bye)

    -- ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô noclip
    toggleNoclip(true)

    -- **‡πÉ‡∏ä‡πâ `task.spawn()` ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÇ‡∏Ñ‡πâ‡∏î‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏ï‡πà‡∏≠‡πÄ‡∏ô‡∏∑‡πà‡∏≠‡∏á‡πÇ‡∏î‡∏¢‡πÑ‡∏°‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°‡∏ã‡πâ‡∏≥**
    task.spawn(function()
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
        local humanoid = character:FindFirstChild("Humanoid")

        local bagList = {}

        while running2 do
            local found = false

            for _, bag in pairs(workspace:GetChildren()) do
                if bag.Name == "TrashBag" and not table.find(bagList, bag) then
                    local prompt = bag:FindFirstChild("QueueInwZa007")
                    if prompt and prompt.Enabled then
                        if humanoidRootPart and humanoid then
                            local targetCFrame = bag:IsA("Model") and bag.PrimaryPart and bag.PrimaryPart.CFrame or bag.CFrame

                            humanoidRootPart.CFrame = targetCFrame * CFrame.new(0, -10, 0)
                            task.wait(0.2)

                            humanoidRootPart.Anchored = true

                            while humanoidRootPart.Position.Y > -100 and running2 do
                                humanoidRootPart.CFrame = targetCFrame * CFrame.new(0, -10, 0)
                                task.wait(0.1)
                            end

                            -- ‡∏Å‡∏î ProximityPrompt ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥
                            prompt:InputHoldBegin()
                            task.wait(prompt.HoldDuration or 0)
                            prompt:InputHoldEnd()

                            humanoidRootPart.Anchored = false

                            table.insert(bagList, bag)

                            found = true

                            -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ó‡∏µ‡∏°‡∏Ç‡∏≠‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÅ‡∏•‡πâ‡∏ß‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏ó‡∏µ‡πà‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°
                            if player.Team then
                                if player.Team.Name == "‡∏ö‡πâ‡∏≤‡∏ô‡∏ù‡∏±‡πà‡∏á‡∏Ñ‡∏•‡∏≠‡∏á‡πÄ‡∏ï‡∏¢" then
                                    -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏à‡∏∏‡∏î‡∏Ç‡∏≠‡∏á‡∏ó‡∏µ‡∏° "‡∏ö‡πâ‡∏≤‡∏ô‡∏ù‡∏±‡πà‡∏á‡∏Ñ‡∏•‡∏≠‡∏á‡πÄ‡∏ï‡∏¢"
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1146.37036, 2.90202761, -399.228149, 0, 0, 1, 0, 1, 0, -1, 0, 0)
                                elseif player.Team.Name == "‡∏ö‡πâ‡∏≤‡∏ô‡∏ù‡∏±‡πà‡∏á‡∏ò‡∏ô" then
                                    -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏à‡∏∏‡∏î‡∏Ç‡∏≠‡∏á‡∏ó‡∏µ‡∏° "‡∏ö‡πâ‡∏≤‡∏ô‡∏ù‡∏±‡πà‡∏á‡∏ò‡∏ô"
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1069.29102, 2.90202761, -403.238678, -0.0349013805, 0, 0.999390781, 0, 1, 0, -0.999390781, 0, -0.0349013805)
                                end
                            end
                        end
                    end
                end
            end

            if not found then
                MAuto.autoF1off2.Visible = false
                MAuto.autoF1on2.Visible = true
                running2 = false
                toggleNoclip(false)

                script.Parent.Bye.byr.TextLabel.Text = "‡∏´‡∏¢‡∏∏‡∏î‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏´‡∏≤‡∏Ç‡∏¢‡∏∞‡πÑ‡∏°‡πà‡πÄ‡∏à‡∏≠"
                moveFrameToNewPosition4(script.Parent.Bye)
                break
            end

            task.wait(0.1)
        end
    end)
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡∏´‡∏¢‡∏∏‡∏î
MAuto.autoF1off2.MouseButton1Click:Connect(function()
    toggleNoclip(false)

    MAuto.autoF1off2.Visible = false
    MAuto.autoF1on2.Visible = true
    running2 = false

    local player = game.Players.LocalPlayer  -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô

    -- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏ó‡∏µ‡∏°‡∏Ç‡∏≠‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÅ‡∏•‡∏∞‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏à‡∏∏‡∏î‡∏ó‡∏µ‡πà‡∏Å‡∏≥‡∏´‡∏ô‡∏î
    if player.Team then
        if player.Team.Name == "‡∏ö‡πâ‡∏≤‡∏ô‡∏ù‡∏±‡πà‡∏á‡∏Ñ‡∏•‡∏≠‡∏á‡πÄ‡∏ï‡∏¢" then
            -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏à‡∏∏‡∏î‡∏Ç‡∏≠‡∏á‡∏ó‡∏µ‡∏° "‡∏ö‡πâ‡∏≤‡∏ô‡∏ù‡∏±‡πà‡∏á‡∏Ñ‡∏•‡∏≠‡∏á‡πÄ‡∏ï‡∏¢"
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1146.37036, 2.90202761, -399.228149, 0, 0, 1, 0, 1, 0, -1, 0, 0)
        elseif player.Team.Name == "‡∏ö‡πâ‡∏≤‡∏ô‡∏ù‡∏±‡πà‡∏á‡∏ò‡∏ô" then
            -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏à‡∏∏‡∏î‡∏Ç‡∏≠‡∏á‡∏ó‡∏µ‡∏° "‡∏ö‡πâ‡∏≤‡∏ô‡∏ù‡∏±‡πà‡∏á‡∏ò‡∏ô"
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1069.29102, 2.90202761, -403.238678, -0.0349013805, 0, 0.999390781, 0, 1, 0, -0.999390781, 0, -0.0349013805)
        end
    end
end)

	---------------------------------------------------------
	
local F1 = me.Parent.Frame1
local player = game.Players.LocalPlayer
local openButton = F1.open
local closeButton = F1.off

local function adjustSize(part, size)
    if part then
        part.Size = size
    end
end

openButton.MouseButton1Click:Connect(function()
    -- ‡∏î‡∏∂‡∏á‡∏Ñ‡πà‡∏≤‡∏à‡∏≤‡∏Å TextBox ‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏ó‡∏µ‡πà‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°
    local hitbox = tonumber(F1.TextBoxsize.Text)  -- ‡πÅ‡∏õ‡∏•‡∏á‡πÄ‡∏õ‡πá‡∏ô‡∏ï‡∏±‡∏ß‡πÄ‡∏•‡∏Ç
    if not hitbox then
        warn("‡∏Ñ‡πà‡∏≤‡∏ó‡∏µ‡πà‡∏Å‡∏£‡∏≠‡∏Å‡πÑ‡∏°‡πà‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á")
        return
    end

    for _, otherPlayer in pairs(game.Players:GetPlayers()) do
        if otherPlayer ~= player then
            local otherCharacter = otherPlayer.Character
            if otherCharacter then
                local otherHumanoidRootPart = otherCharacter:FindFirstChild("HumanoidRootPart")
                if otherHumanoidRootPart then
                    otherHumanoidRootPart.Transparency = 0.8
                    adjustSize(otherHumanoidRootPart, Vector3.new(hitbox, hitbox, hitbox))
                end
            end
        end
    end
end)

closeButton.MouseButton1Click:Connect(function()
    for _, otherPlayer in pairs(game.Players:GetPlayers()) do
        if otherPlayer ~= player then
            local otherCharacter = otherPlayer.Character
            if otherCharacter then
                local otherHumanoidRootPart = otherCharacter:FindFirstChild("HumanoidRootPart")
                if otherHumanoidRootPart then
                    otherHumanoidRootPart.Transparency = 1
                    adjustSize(otherHumanoidRootPart, Vector3.new(2, 2, 1))
                end
            end
        end
    end
end)

	
	
end
coroutine.wrap(LTVFGO_fake_script)()
